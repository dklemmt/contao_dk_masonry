{
    "name": "dklemmt/contao_dk_masonry",
    "description": "Implementation of Masonry as extension for Contao Open Source CMS",
    "keywords": ["contao", "gallery", "grid", "masonry"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dirk Klemmt",
            "email": "dirch@quantentunnel.de",
            "role": "Developer"
        },
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dirch@quantentunnel.de",
        "issues": "https://github.com/dklemmt/contao_dk_masonry/issues",
        "source": "https://github.com/dklemmt/contao_dk_masonry"
    },
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dirch\\ContaoMasonry\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Dirch\\ContaoMasonry\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
